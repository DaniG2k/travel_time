<!DOCTYPE html>
<html>
  <head>
    <title>TravelTime</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% flash.each do |key, msg| %>
      <div class="alert alert-<%= key %>">
        <%= msg %>
      </div>
    <% end %>
    
    <%= yield %>
  </body>
</html>
